{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": ["airbnb-typescript-prettier"],
  "rules": {
    "react/prop-types": "off",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "import/prefer-default-export": "off",
    "no-plusplus": "off"
  }
}
